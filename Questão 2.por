programa {
  funcao inicio() {
    inteiro n1
    inteiro n2
    escreva("Digite o primeiro numero: ")
    leia(n1)
    limpa()
    escreva("Digite o segundo numero ")
    leia(n2)
    limpa()
    escreva("A soma dos numeros é " + (n1 + n2))
    escreva("\n")
    escreva("A subtração dos numeros é " + (n1 - n2))
    escreva("\n")
    escreva("A multipliacação dos numeros é " + (n1 * n2))
  }
}
